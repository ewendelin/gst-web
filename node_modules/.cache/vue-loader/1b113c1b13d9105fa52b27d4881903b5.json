t\t\t\t\t\t}}</v-list-item-title>\n\t\t\t\t\t</v-list-item-content>\n\t\t\t\t\t<v-list-item-icon>\n\t\t\t\t\t\t<v-btn fab text>\n\t\t\t\t\t\t\t<v-icon>mdi-logout</v-icon>\n\t\t\t\t\t\t</v-btn>\n\t\t\t\t\t</v-list-item-icon>\n\t\t\t\t</v-list-item>\n\t\t\t</v-list>\n\n\t\t\t<h3 class=\"mx-4 mt-3\">Today's claims</h3>\n\n\t\t\t<v-list\n\t\t\t\trow\n\t\t\t\ttwo-line\n\t\t\t\tv-for=\"(deal, index) in deals\"\n\t\t\t\t:key=\"index\"\n\t\t\t\twidth=\"90vw\"\n\t\t\t>\n\t\t\t\t<v-list-item>\n\t\t\t\t\t<v-list-item-content>\n\t\t\t\t\t\t<v-list-item-title>\n\t\t\t\t\t\t\t{{ deal.promotion.name }}\n\t\t\t\t\t\t</v-list-item-title>\n\t\t\t\t\t\t<v-list-item-subtitle>\n\t\t\t\t\t\t\t{{ deal.promotion.end_time }}\n\t\t\t\t\t\t</v-list-item-subtitle>\n\t\t\t\t\t</v-list-item-content>\n\t\t\t\t\t<v-spacer></v-spacer>\n\t\t\t\t\t<v-list-item-icon @click=\"deal.dialog = true\" class=\"my-0\">\n\t\t\t\t\t\t<v-btn fab text>\n\t\t\t\t\t\t\t<v-icon>mdi-delete</v-icon>\n\t\t\t\t\t\t</v-btn>\n\t\t\t\t\t\t<v-dialog v-model=\"deal.dialog\" max-width=\"290\">\n\t\t\t\t\t\t\t<v-card>\n\t\t\t\t\t\t\t\t<v-layout row class=\"mx-auto\">\n\t\t\t\t\t\t\t\t\t<v-card-title class=\"headline\">Cancel coupon?</v-card-title>\n\t\t\t\t\t\t\t\t\t<v-spacer></v-spacer>\n\t\t\t\t\t\t\t\t\t<v-btn icon @click=\"deal.dialog = false\">\n\t\t\t\t\t\t\t\t\t\t<v-icon>mdi-close</v-icon>\n\t\t\t\t\t\t\t\t\t</v-btn>\n\t\t\t\t\t\t\t\t</v-layout>\n\n\t\t\t\t\t\t\t\t<v-card-text>\n\t\t\t\t\t\t\t\t\tAre you sure you want to cancel this deal? They will no longer\n\t\t\t\t\t\t\t\t\tbe expecting you!\n\t\t\t\t\t\t\t\t</v-card-text>\n\n\t\t\t\t\t\t\t\t<v-card-actions class=\"d-flex justify-center pb-3\">\n\t\t\t\t\t\t\t\t\t<v-btn\n\t\t\t\t\t\t\t\t\t\twidth=\"80%\"\n\t\t\t\t\t\t\t\t\t\tdark\n\t\t\t\t\t\t\t\t\t\tcolor=\"#DFA937\"\n\t\t\t\t\t\t\t\t\t\ttile\n\t\t\t\t\t\t\t\t\t\tclass=\"buttons\"\n\t\t\t\t\t\t\t\t\t\tdepressed\n\t\t\t\t\t\t\t\t\t\t@click=\"remove(deal.id, index)\"\n\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\tyes, i am sure\n\t\t\t\t\t\t\t\t\t</v-btn>\n\t\t\t\t\t\t\t\t</v-card-actions>\n\t\t\t\t\t\t\t</v-card>\n\t\t\t\t\t\t</v-dialog>\n\t\t\t\t\t</v-list-item-icon>\n\t\t\t\t</v-list-item>\n\t\t\t</v-list>\n\t\t</v-layout>\n\t\t<v-layout justify-center>\n\t\t\t<v-btn\n\t\t\t\twidth=\"80%\"\n\t\t\t\tdark\n\t\t\t\tcolor=\"#DFA937\"\n\t\t\t\tclass=\"buttons mb-12\"\n\t\t\t\ttile\n\t\t\t\tdepressed\n\t\t\t\tto=\"/deals\"\n\t\t\t>\n\t\t\t\tbrowse deals\n\t\t\t</v-btn>\n\t\t</v-layout>\n\t\t<v-footer color=\"white\" padless class=\"mb-12\">\n\t\t\t<v-row justify=\"center\" no-gutters>\n\t\t\t\t<p class=\"caption ma-2 pt-3 text-center\">\n\t\t\t\t\tLorem ipsum dolor sit amet, consectetur adipisicing elit. Pariatur,\n\t\t\t\t\trerum! Modi illo deserunt excepturi similique ea magni.\n\t\t\t\t</p>\n\t\t\t\t<v-col class=\"#FFF py-4 text-center gray--text\" cols=\"12\">\n\t\t\t\t\t{{ new Date().getFullYear() }} — <strong>Gäst</strong>\n\t\t\t\t</v-col>\n\t\t\t</v-row>\n\t\t</v-footer>\n\t</div>\n</template>\n<script>\n\timport axios from 'axios';\n\timport Navbar from '../components/Navbar';\n\n\texport default {\n\t\tname: 'Profile',\n\t\tcomponents: { Navbar },\n\t\tmounted() {\n\t\t\tthis.getProfileInfo();\n\t\t},\n\t\tcreated() {},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tcustomerID: 1,\n\t\t\t\tdeals: [],\n\t\t\t\tsnackbar: false,\n\t\t\t\tusers: [\n\t\t\t\t\t{\n\t\t\t\t\t\tusername: 'Jim Halpert',\n\t\t\t\t\t\tavatar: 'https://cdn.vuetifyjs.com/images/cards/foster.jpg'\n\t\t\t\t\t}\n\t\t\t\t]\n\t\t\t};\n\t\t},\n\t\tmethods: {\n\t\t\tgetProfileInfo() {\n\t\t\t\taxios\n\t\t\t\t\t.get(\n\t\t\t\t\t\t`http://localhost:3000/api/v1/customers/${this.customerID}/show_coupons`\n\t\t\t\t\t)\n\t\t\t\t\t.then(response => {\n\t\t\t\t\t\tthis.deals = response.data;\n\t\t\t\t\t\tthis.addDialog();\n\t\t\t\t\t})\n\t\t\t\t\t.catch(function(error) {\n\t\t\t\t\t\talert('fail' + error);\n\t\t\t\t\t});\n\t\t\t},\n\t\t\tremove(deal_id, index) {\n\t\t\t\t// this.deals.splice(id, 1); JavaScript\n\t\t\t\t// below VueJS delete helper\n\t\t\t\tthis.$delete(this.deals, index);\n\t\t\t\taxios\n\t\t\t\t\t.put(\n\t\t\t\t\t\t`http://localhost:3000/api/v1/customers/${this.customerID}/cancel_coupon`,\n\t\t\t\t\t\t{\n\t\t\t\t\t\t\tdeal_id: deal_id\n\t\t\t\t\t\t}\n\t\t\t\t\t)\n\t\t\t\t\t.then(response => {\n\t\t\t\t\t\tconsole.log('canceled');\n\t\t\t\t\t})\n\t\t\t\t\t.catch(function(error) {\n\t\t\t\t\t\talert('fail' + error);\n\t\t\t\t\t});\n\t\t\t},\n\t\t\taddDialog() {\n\t\t\t\tthis.deals = this.deals.map(deals => ({\n\t\t\t\t\t...deals,\n\t\t\t\t\tdialog: false\n\t\t\t\t}));\n\t\t\t}\n\t\t}\n\t};\n</script>\n\n<style scoped>\n\t.profile {\n\t\tbackground-color: white;\n\t}\n\t.buttons {\n\t\tborder-radius: 5px;\n\t}\n</style>\n"]}]}